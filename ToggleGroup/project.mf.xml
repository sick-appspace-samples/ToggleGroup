<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<manifest>
    <application name="ToggleGroup">
        <crown name="ToggleGroup">
            <desc>user script</desc>
            <serves>
                <event name="OptionSwitched">
                    <desc/>
                    <param desc="" name="value" type="string"/>
                </event>
                <function name="selectedOptionChanged">
                    <desc/>
                    <param desc="" name="option" type="string"/>
                </function>
                <function name="getSelectedOption">
                    <desc/>
                    <return desc="" name="option" type="string"/>
                </function>
            </serves>
        </crown>
        <meta key="author">AppSpace Sample</meta>
        <meta key="version">1.0</meta>
        <meta key="priority">low</meta>
        <meta key="read-protected">false</meta>
        <meta key="copy-protected">false</meta>
        <entry default="main.lua" path="scripts"/>
    </application>
</manifest>
